<!-- settings.xml for Magnet public repo -->
<settings>
  <profiles>
    <profile>
      <id>magnet-repo-public</id>
      <repositories>
        <repository>
          <id>mmx</id>
          <url>https://repo.magnet.com/artifactory/public</url>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <id>central-https</id>
          <url>https://repo1.maven.org/maven2</url>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>
        </pluginRepository>
        <pluginRepository>
          <id>gradle-plugin</id>
          <url>https://repo.gradle.org/gradle/libs-releases-local</url>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>
        </pluginRepository>
        <pluginRepository>
          <id>magnet-plugin</id>
          <url>https://repo.magnet.com/artifactory/public</url>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>
        </pluginRepository>
        <pluginRepository>
          <id>reucon-snapshots</id>
          <url>https://secure.reucon.net/nexus/content/repositories/opensource-snapshots</url>
          <releases>
            <enabled>false</enabled>
          </releases>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>
        </pluginRepository>
        <pluginRepository>
          <id>reucon-release</id>
          <url>https://secure.reucon.net/nexus/content/repositories/opensource</url>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
        </pluginRepository>
      </pluginRepositories>
    </profile>
  </profiles>
  <activeProfiles>
    <activeProfile>magnet-repo-public</activeProfile>
  </activeProfiles>
</settings>
